{
    "name":"newstring",
    "version":"1.0.0",
    "manifest_version": 3,
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "action":{
        "default_popup":"popup.html",
        "default_title":"Timeline"
    },
    "permissions": [
        "webRequest",
        "storage",
        "activeTab",
        "cookies"
    ],
    "host_permissions": [
        "*://*.naver.com/*"
    ],
    "icons": { 
        "16": "./icon/logo_16_icon.png",
        "48": "./icon/logo_48_icon.png",
       "128": "./icon/logo_128_icon.png"
    }

}