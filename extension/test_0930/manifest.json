{
    "name":"newstring",
    "version":"1.0.0",
    "manifest_version": 3,
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "action":{
        "default_popup":"popup.html",
        "default_title":"Timeline"
    },
    "permissions": [
        "webRequest",
        "storage",
        "activeTab",
        "cookies"
    ],
    "host_permissions": [
        "*://*.naver.com/*"
      ]
}